{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Serilog:Properties": {
    "Application": "PinIQ Development"
  },
  "Authentication": {
    "Schemes": {
      "Bearer": {
        "ValidAudiences": [
          "https://localhost:5001",
          "http://localhost:5000",
          "http://localhost:8000",
          "https://localhost:8001"
        ],
        "ValidIssuer": "piniq-dev",
        "AccessTokenExpirationMinutes": 30,
        "RefreshTokenExpirationDays": 3,
        "RefreshTokenRenewalWindowDays": 3
      }
    }
  },
  "DeveloperOptions": {
    "LoginTestKey": "thisIsATest"
  }
}